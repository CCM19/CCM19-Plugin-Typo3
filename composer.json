{
	"name": "ccm19/typo3-plugin",
	"description": "CCM19 (https://www.ccm19.de/) Cookie-Banner with Data-Protection Plugin",
	"type": "typo3-cms-extension",
	"minimum-stability": "stable",
	"license": "GPL-2.0+",
	"version": "1.0.0",
	"authors": [
		{
			"name": "ccm19",
			"email": "info@ccm19.de",
			"homepage": "https://www.ccm19.de"

		}
	],
	"require": {
		"typo3/cms-core": "^12.4 || ^13.0",
		"typo3/cms-backend": "^12.4 || ^13.0",
		"php": "^8.1"
	},
	"autoload": {
		"psr-4": {
			"ccm19\\typo3Plugin\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ccm19"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	}
}
